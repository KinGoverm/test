    <script language="JavaScript">
        
        var h =13;
        var m =11;
        var s =50;

        var gizmo;

        function stopClock() {
            clearTimeout(gizmo);
        }

        function yourClock() {
            var time = " ";
            s = parseInt(s) + 1;
            if (s == 60) {
                s = 0;
                m = parseInt(m) + 1;
                if (m == 60) {
                    m = 0;
                    h = parseInt(h) + 1;
                    if (h == 24) {
                        h = 0;
                    }
                }
            }
            time += h + ":" + m + ":" + s;
            document.getElementById("the_time").innerHTML = time;
            gizmo = setTimeout("yourClock()", 1000);
        }

    </script>
