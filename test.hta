                    list.append('<li><span style="float:left;">' + data.result.deliverDate + '</span><span style="float:right;">' + data.result.deliverTime + '</span><br style="clear:both;" /></li>\n');
                    list.append('<li><span style="text-align:center;display:block;font-weight:bold;">' + data.result.username + '</span></li>\n');
