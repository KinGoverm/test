    return validateOptions();
}

function validateOptions(){


    var message = '\n\n';
    var valid= true;
    $('select').each(function() {
        if($(this).val() == null ||$(this).val() == '' || $(this).val() == '-1') {
            if($(this).is(':visible') == true) {
                var label = $('label[for="' + $(this).attr('id') + '"]');
                var msg = '';
                if (label.html()!= null && label.html().indexOf('<') != -1) {
                    msg = label.html().substring(0, label.html().indexOf('<')) + '\n';
                    valid = false;
                }
                message += msg;
            }
        }
    });
    $('input').each(function() {
        if( $(this).val() == null ||$(this).val().trim() == '') {
            if($(this).is(':visible') == true) {
                var label = $('label[for="' + $(this).attr('id') + '"]');
                var msg = '';
                if (label.html() != null && label.html().indexOf('<') != -1) {
                    msg = label.html().substring(0, label.html().indexOf('<')) + '\n';
                    valid = false;
                }
                message += msg;
            }
        }
    });
    $('textarea').each(function() {
        if( $(this).val() == null ||$(this).val().trim() == '') {
            if($(this).is(':visible') == true) {
                var label = $('label[for="' + $(this).attr('id') + '"]');
                var msg = '';
                if (label.html() != null && label.html().indexOf('<') != -1) {
                    msg = label.html().substring(0, label.html().indexOf('<')) + '\n';
                    valid = false;
                }
                message += msg;
            }
        }
    });
    if(!valid){
        alert('\u0645\u0648\u0627\u0631\u062F \u0632\u06CC\u0631 \u0627\u0644\u0632\u0627\u0645\u06CC \u0627\u0633\u062A\u060C \u0622\u0646\u0647\u0627 \u0631\u0627 \u062A\u06A9\u0645\u06CC\u0644 \u0646\u0645\u0627\u06CC\u06CC\u062F:' + message );
    }
    return valid;
}







</script>



    
    
    

